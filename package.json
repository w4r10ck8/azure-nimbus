{
  "name": "azure-report-cli",
  "version": "1.0.0",
  "description": "A CLI tool to get information from Azure resources",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "azure-report": "dist/index.js"
  },
  "scripts": {
    "build": "bun run tsc",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "postbuild": "chmod +x dist/index.js"
  },
  "keywords": ["azure", "cli", "reporting", "cloud"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/arm-resources": "^5.0.0",
    "@azure/arm-subscriptions": "^5.0.0",
    "commander": "^11.0.0",
    "chalk": "^5.0.0",
    "ora": "^7.0.0",
    "cli-progress": "^3.12.0",
    "inquirer": "^9.0.0"
  }
}